{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-2",
    "es2015"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "syntax-jsx",
    "jsx-v-model",
    "transform-vue-jsx"
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "stage-2"
      ],
      "plugins": [
        "istanbul",
        "transform-vue-jsx",
        "dynamic-import-node"
      ]
    }
  }
}
